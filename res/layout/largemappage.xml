<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
    <com.baidu.mapapi.map.MapView android:id="@+id/largeMapMap"
                                  android:layout_width="fill_parent"
                                  android:layout_height="fill_parent"
                                  android:clickable="true" android:layout_alignParentLeft="true"
                                  android:layout_alignParentTop="true"
            />
    <Button
            android:layout_width="55dp"
            android:layout_height="55dp"
            android:id="@+id/largeMapBtnReturn" android:layout_alignParentLeft="true" android:layout_marginLeft="10dp"
            android:layout_alignParentTop="true" android:layout_marginTop="6dp" android:background="@drawable/back_bg"/>
</RelativeLayout>